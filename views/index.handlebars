<div class="container">
  {{#each ics }}
  <img src="{{image}}" class="car-image" />
  <table>
    <tr>
      <th>{{name}}</th>
    </tr>
    <tr>
      <td>MRSP: <span> ${{msrp}}</span></td>
    </tr>
    <tr>
      <td>HorsePower: <span> {{engine}}</span></td>
    </tr>
    <tr>
      <td>0/100KM/H: <span> {{speed}}</span></td>
    </tr>
    <tr>
      <td>Transmission: <span> {{transmission}}</span></td>
    </tr>
  </table>
  <br>
   <a class="waves-effect waves-light btn">TECH SPECS</a>
    <a class="waves-effect waves-light btn">RENT CAR</a>
    <br><br>
  {{/each}}

</div>