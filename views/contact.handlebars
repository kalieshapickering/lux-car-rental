<br><br>
<div class="container">
  <div class="row">
    <div class="arrow">
      <div class="line"></div>
    </div>
    <h5><strong class="newTitle">CONTACT US</strong></h5>
    <div id="contact-us"></div>
  </div>
</div>
</div>
<br>
<div class="container">
  <div class="card amber accent-2">
    <form class="col s8 contactUs" style="display: inline">
      {{!-- <div class="row"> --}}
        <div class="input-field col s4">
          <input placeholder="Placeholder" id="firstInput" type="text" class="validate">
          <label for="firstInput">First Name</label>
        </div>
        <div class="input-field col s4">
          <input id="lastNameInput" type="text" class="validate">
          <label for="lastNameInput">Last Name</label>
        </div>
        {{!-- <div class="row"> --}}
          <div class="input-field col s8">
            <input id="phoneNumberInput" type="number" class="validate">
            <label for="phoneNumberInput">Phone Number</label>
          </div>
          {{!-- <div class="row"> --}}
            <div class="input-field col s8">
              <input id="emailInput" type="email" class="validate">
              <label for="emailInput">Email</label>
            </div>
            {{!-- <div class="row"> --}}
              <div class="input-field col s8">
                <textarea id="messageInput" class="materialize-textarea" length="200"></textarea>
                <label for="messageInput">Message</label>
              </div>
              <button class="btn waves-effect waves-light" id="submit-btn" type="submit" name="action">Submit</button>
    </form>
  </div>
</div>
</div>